def add(a,b):
  answer = a + b 
  return answer 

def subtract(a,b):
  answer = a - b
  return answer 

def multiply(a,b):
  answer = a * b 
  return answer 

def divide(a,b):
  answer = a/b
  return answer

def exp(a,b):
  answer = a**b
  return answer 


print(add(3,4))
print(subtract(3,4))
print(multiply(3,4))
print(divide(3,4))
print(exp(3,4))
